{
	"if": {
		"allOf": [
			{
				"field": "type",
				"equals": "Microsoft.Network/expressRouteCircuits"
			},
			{
				"not": {
					"field": "Microsoft.Network/expressRouteCircuits/serviceProvider.bandwidthInMbps",
					"in": "[parameters('listOfBandwidthinMbps')]"
				}
			}
		]
	},
	"then": {
		"effect": "Deny"
	}
}