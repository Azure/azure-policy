{
	"if": {
		"allOf": [
			{
				"field": "type",
				"in": [
					"Microsoft.Compute/virtualMachines",
					"Microsoft.Compute/VirtualMachineScaleSets"
				]
			},
			{
				"field": "Microsoft.Compute/imagePublisher",
				"equals": "redhat"
			},
			  {
				"field": "Microsoft.Compute/imageOffer",
				"equals": "rhel-byos"
			},
			{
				"field": "Microsoft.Compute/imageSKU",
				"in": [
					"rhel-lvm74",
					"rhel-lvm75",
					"rhel-lvm76",
					"rhel-lvm76-gen2",
					"rhel-lvm77",
					"rhel-lvm77-gen2",
					"rhel-lvm78",
					"rhel-lvm78-gen2",
					"rhel-lvm79",
					"rhel-lvm79-gen2",
					"rhel-lvm8",
					"rhel-lvm8-gen2",
					"rhel-lvm81",
					"rhel-lvm81-gen2",
					"rhel-lvm82",
					"rhel-lvm82-gen2",
					"rhel-lvm83",
					"rhel-lvm83-gen2",
					"rhel-lvm84",
					"rhel-lvm84-gen2",
					"rhel-lvm85",
					"rhel-lvm85-gen2",
					"rhel-raw-ci76",
					"rhel-raw69",
					"rhel-raw72",
					"rhel-raw75",
					"rhel-raw76"
				]
			},
			{
				"field": "Microsoft.Compute/licenseType",
				"notEquals": "RHEL_BYOS"
			}
		]
	},
	"then": {
		"effect": "deny"
	}
}