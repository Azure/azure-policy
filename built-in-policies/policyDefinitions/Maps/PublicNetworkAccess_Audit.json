{
  "properties": {
    "displayName": "Public network access should be disabled for Azure Maps Accounts.",
    "policyType": "BuiltIn",
    "mode": "Indexed",
    "description": "Disabling public network access on a Maps Account improves security by ensuring your Maps Account can only be accessed from a private endpoint.",
    "metadata": {
      "version": "1.0.0",
      "category": "Maps"
    },
    "version": "1.0.0",
    "parameters": {
      "effect": {
        "type": "String",
        "metadata": {
          "displayName": "Effect",
          "description": "Enable or disable the execution of the policy"
        },
        "allowedValues": [
          "Audit",
          "Deny",
          "Disabled"
        ],
        "defaultValue": "Audit"
      }
    },
    "policyRule": {
      "if": {
        "allOf": [
          {
            "field": "type",
            "equals": "Microsoft.Maps/accounts"
          },
          {
            "field": "Microsoft.Maps/accounts/publicNetworkAccess",
            "notEquals": "Disabled"
          }
        ]
      },
      "then": {
        "effect": "[parameters('effect')]"
      }
    },
    "versions": [
      "1.0.0"
    ]
  },
  "id": "/providers/Microsoft.Authorization/policyDefinitions/a46d869a-f759-4404-8289-7737e7a1f79e",
  "name": "a46d869a-f759-4404-8289-7737e7a1f79e"
}